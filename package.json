{
  "name": "@eik/node-client",
  "version": "1.0.0-next.2",
  "description": "A node.js client for interacting with a Eik server.",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "CHANGELOG.md",
    "package.json",
    "LICENSE",
    "dist",
    "src"
  ],
  "scripts": {
    "test": "tap --no-esm test/*.js --no-coverage",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prepare": "npm run -s build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eik-lib/rollup-plugin.git"
  },
  "keywords": [
    "eik",
    "esm"
  ],
  "author": "Finn.no",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eik-lib/node-client/issues"
  },
  "homepage": "https://github.com/eik-lib/node-client#readme",
  "dependencies": {
    "@eik/common": "4.0.0-next.4",
    "abslog": "2.4.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.2",
    "eslint": "7.21.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "prettier": "2.2.1",
    "rollup": "2.40.0",
    "semantic-release": "17.4.1",
    "tap": "14.11.0"
  }
}
