{
  "name": "@eik/node-client",
  "version": "1.1.39",
  "description": "A node.js client for interacting with a Eik server.",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "CHANGELOG.md",
    "package.json",
    "LICENSE",
    "dist",
    "src"
  ],
  "scripts": {
    "test": "tap --no-coverage",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prepare": "npm run -s build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eik-lib/node-client.git"
  },
  "keywords": [
    "eik",
    "esm"
  ],
  "author": "Finn.no",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eik-lib/node-client/issues"
  },
  "homepage": "https://github.com/eik-lib/node-client#readme",
  "dependencies": {
    "@eik/common": "3.0.1",
    "abslog": "2.4.0",
    "undici": "5.17.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "eslint": "8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.3",
    "rollup": "2.79.1",
    "semantic-release": "19.0.5",
    "tap": "16.3.4",
    "@babel/eslint-parser": "7.19.1"
  }
}
